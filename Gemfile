

source "https://rubygems.org"

# Jekyll itself
gem "jekyll", "~> 4.4"

gem "github-pages", group: :jekyll_plugins

# Theme
gem "minima", "~> 2.5"

# Jekyll plugins (optional, but useful)
gem "jekyll-feed", "~> 0.16"      # For RSS feed
gem "jekyll-seo-tag", "~> 2.8"    # SEO metadata

# For Windows file system notifications (optional, recommended on Windows)
gem "wdm", "~> 0.1", :platforms => [:mswin, :mingw, :x64_mingw]

gem "webrick"     
